<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>description</key>
		<string>This patch rounds a numerical value to the nearest, largest or smallest integer.</string>
		<key>name</key>
		<string>Round</string>
		<key>aliasNames</key>
		<array>
			<string>Ceil</string>
			<string>Floor</string>
		</array>
		<key>categories</key>
		<array>
			<string>Utility/Numeric</string>
		</array>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputValue</key>
		<dict>
			<key>description</key>
			<string>The initial value.</string>
			<key>name</key>
			<string>Value</string>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputRound</key>
		<dict>
			<key>description</key>
			<string>The value rounded to the nearest integer.</string>
			<key>name</key>
			<string>Rounded Value</string>
		</dict>
		<key>outputCeil</key>
		<dict>
			<key>description</key>
			<string>The value rounded to the largest integer.</string>
			<key>name</key>
			<string>Ceil Value</string>
		</dict>
		<key>outputFloor</key>
		<dict>
			<key>description</key>
			<string>The value rounded to the smallest integer.</string>
			<key>name</key>
			<string>Floor Value</string>
		</dict>
	</dict>
	
</dict>
</plist>
