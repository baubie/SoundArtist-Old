<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>categories</key>
		<array>
			<string>Interaction</string>
		</array>
		<key>name</key>
		<string>Inertia</string>
		<key>aliasNames</key>
		<array>
			<string>Physics</string>
			<string>Acceleration</string>
			<string>Velocity</string>
			<string>Bounce</string>
			<string>Force</string>
		</array>
		<key>description</key>
		<string>This patch applies the principles of inertia to a value with an optional amount of friction.  Note that this must be used within a feedback loop to work properly.</string>
        <key>examples</key>
        <array>
            <string>Inertia.qtz</string>
            <string>Inertia with Limits.qtz</string>
        </array> 
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputValue</key>
		<dict>
			<key>description</key>
			<string>The value to simulate.</string>
			<key>name</key>
			<string>Value</string>
		</dict>
		<key>inputForce</key>
		<dict>
			<key>description</key>
			<string>The force applied to the particle.</string>
			<key>name</key>
			<string>Force</string>
		</dict>
		<key>inputFriction</key>
		<dict>
			<key>description</key>
			<string>The friction value.</string>
			<key>name</key>
			<string>Friction</string>
		</dict>
		<key>inputSampling</key>
		<dict>
			<key>description</key>
			<string>Controls the sampling of the value.</string>
			<key>name</key>
			<string>Sampling</string>
		</dict>
		<key>operandName</key>
		<string>Operand #%i</string>
		<key>operandDescription</key>
		<string>The operand value.</string>
	</dict>

	<key>_valueAttributes</key>
	<dict>
		<key>inputName</key>
		<string>Value #%i</string>
		<key>inputDescription</key>
		<string>The value to simulate.</string>
		<key>forceName</key>
		<string>Force #%i</string>
		<key>forceDescription</key>
		<string>Extra force applied to particle.</string>
		<key>outputName</key>
		<string>Output Value #%i</string>
		<key>outputDescription</key>
		<string>The simulated value.</string>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputValue</key>
		<dict>
			<key>description</key>
			<string>The simulated value.</string>
			<key>name</key>
			<string>Simulated Value</string>
		</dict>
	</dict>
	
</dict>
</plist>
