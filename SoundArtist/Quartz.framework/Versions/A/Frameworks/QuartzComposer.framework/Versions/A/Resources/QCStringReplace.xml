<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>categories</key>
		<array>
			<string>Utility/String</string>
		</array>
		<key>name</key>
		<string>String Replace</string>
		<key>aliasNames</key>
		<array>
			<string>Search</string>
			<string>Find</string>
			<string>Pattern</string>
		</array>
		<key>description</key>
		<string>This patch replaces occurences of a substring within a string by another string.</string>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputSourceString</key>
		<dict>
			<key>description</key>
			<string>The source string.</string>
			<key>name</key>
			<string>String</string>
		</dict>
		<key>inputSearchString</key>
		<dict>
			<key>description</key>
			<string>The substring to search for.</string>
			<key>name</key>
			<string>Search String</string>
		</dict>
		<key>inputReplaceString</key>
		<dict>
			<key>description</key>
			<string>The string to replace the found substrings with.</string>
			<key>name</key>
			<string>Replace String</string>
		</dict>
		<key>inputCaseSensitive</key>
		<dict>
			<key>description</key>
			<string>Indicates if search should be case-sensitive.</string>
			<key>name</key>
			<string>Case Sensitive</string>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputString</key>
		<dict>
			<key>description</key>
			<string>The processed string.</string>
			<key>name</key>
			<string>String</string>
		</dict>
	</dict>
	
</dict>
</plist>
