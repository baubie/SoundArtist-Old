<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>categories</key>
		<array>
			<string>Utility/Composition Repository</string>
		</array>
		<key>name</key>
		<string>Composition Info</string>
		<key>aliasNames</key>
		<array>
			<string>Inputs</string>
			<string>Outputs</string>
		</array>
		<key>description</key>
		<string>This patch loads a Quartz composition from a Composition Repository identifier, a URL or file path, and returns its metadata and list of inputs / outputs.</string>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputPath</key>
		<dict>
			<key>description</key>
			<string>The URL or file path to the Quartz composition file to load.</string>
			<key>name</key>
			<string>Composition Location</string>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputMetadata</key>
		<dict>
			<key>description</key>
			<string>The attributes of the composition.</string>
			<key>name</key>
			<string>Attributes</string>
		</dict>
		<key>outputInputList</key>
		<dict>
			<key>description</key>
			<string>The list of input keys of the composition.</string>
			<key>name</key>
			<string>Input Keys</string>
		</dict>
		<key>outputOutputList</key>
		<dict>
			<key>description</key>
			<string>The list of output keys of the composition.</string>
			<key>name</key>
			<string>Output Keys</string>
		</dict>
	</dict>
	
</dict>
</plist>
