<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>name</key>
		<string>Color Channels</string>
		<key>description</key>
		<string>Controls which color channels the subpatches can write to.</string>
		<key>aliasNames</key>
		<array>
			<string>Mask</string>
		</array>
		<key>categories</key>
		<array>
			<string>- Private</string>
		</array>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputRed</key>
		<dict>
			<key>description</key>
			<string>Enables writing to the red color channel.</string>
			<key>name</key>
			<string>Red Channel</string>
		</dict>
		<key>inputGreen</key>
		<dict>
			<key>description</key>
			<string>Enables writing to the green color channel.</string>
			<key>name</key>
			<string>Green Channel</string>
		</dict>
		<key>inputBlue</key>
		<dict>
			<key>description</key>
			<string>Enables writing to the blue color channel.</string>
			<key>name</key>
			<string>Blue Channel</string>
		</dict>
		<key>inputAlpha</key>
		<dict>
			<key>description</key>
			<string>Enables writing to the alpha color channel.</string>
			<key>name</key>
			<string>Alpha Channel</string>
		</dict>
	</dict>
	
</dict>
</plist>
