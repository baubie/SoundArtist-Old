				<string>Integrator</string>
				<key>aliasNames</key>
				<array>
					<string>Sum</string>
				</array>
				<key>description</key>
				<string>This patch integrates a numerical value over time.</string>
			</dict>
			<key>outputAttributes</key>
			<dict>
				<key>outputValue</key>
				<dict>
					<key>description</key>
