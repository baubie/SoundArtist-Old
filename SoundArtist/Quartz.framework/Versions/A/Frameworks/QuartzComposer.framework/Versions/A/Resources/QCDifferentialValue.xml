		<dict>
			<key>description</key>
			<string>The source value.</string>
			<key>name</key>
			<string>Value</string>
		</dict>
		<key>inputResetSignal</key>
		<dict>
			<key>description</key>
			<string>The reset signal.</string>
			<key>name</key>
			<string>Reset Signal</string>
		</dict>
	</dict>
	
