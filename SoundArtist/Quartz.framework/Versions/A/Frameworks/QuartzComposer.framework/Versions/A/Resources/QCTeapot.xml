<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>categories</key>
		<array>
			<string>Render</string>
		</array>
		<key>description</key>
		<string>This patch renders the famous OpenGL 3D teapot.

The image on the teapot is multiplied by the color set on the "Color" input.</string>
		<key>name</key>
		<string>Teapot</string>
		<key>aliasNames</key>
		<array>
			<string>GLSL</string>
		</array>
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputX</key>
		<dict>
			<key>description</key>
			<string>Position on the X-axis.</string>
			<key>name</key>
			<string>X Position</string>
		</dict>
		<key>inputY</key>
		<dict>
			<key>description</key>
			<string>Position on the Y-axis.</string>
			<key>name</key>
			<string>Y Position</string>
		</dict>
		<key>inputZ</key>
		<dict>
			<key>description</key>
			<string>Position on the Z-axis.</string>
			<key>name</key>
			<string>Z Position</string>
		</dict>
		<key>inputRX</key>
		<dict>
			<key>description</key>
			<string>Rotation on the X-axis.</string>
			<key>name</key>
			<string>X Rotation</string>
		</dict>
		<key>inputRY</key>
		<dict>
			<key>description</key>
			<string>Rotation on the Y-axis.</string>
			<key>name</key>
			<string>Y Rotation</string>
		</dict>
		<key>inputRZ</key>
		<dict>
			<key>description</key>
			<string>Rotation on the Z-axis.</string>
			<key>name</key>
			<string>Z Rotation</string>
		</dict>
		<key>inputScale</key>
		<dict>
			<key>description</key>
			<string>The scale of the teapot.</string>
			<key>name</key>
			<string>Scale</string>
		</dict>
		<key>inputColor</key>
		<dict>
			<key>description</key>
			<string>The color of the teapot.</string>
			<key>name</key>
			<string>Color</string>
		</dict>
		<key>inputImage</key>
		<dict>
			<key>description</key>
			<string>The image on the teapot.</string>
			<key>name</key>
			<string>Image</string>
		</dict>
	</dict>
	
</dict>
</plist>
