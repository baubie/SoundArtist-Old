<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>description</key>
		<string>Generates a signal whenever the value or object on its input port is updating.</string>
		<key>name</key>
		<string>Watcher</string>
		<key>aliasNames</key>
		<array>
			<string>Signal</string>
			<string>Pulse</string>
			<string>Update</string>
		</array>
		<key>categories</key>
		<array>
			<string>Utility/Logic</string>
		</array>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputSignal</key>
		<dict>
			<key>description</key>
			<string>The input port value updated signal.</string>
			<key>name</key>
			<string>Updating Signal</string>
		</dict>
	</dict>
	
	<key>_inAttributes</key>
	<dict>
		<key>name</key>
		<string>Input</string>
		<key>description</key>
		<string>The input port.</string>
	</dict>
	
	<key>_nameFormat</key>
	<string>%@ Watcher</string>
	
</dict>
</plist>
