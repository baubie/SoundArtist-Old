	<dict>
		<key>categories</key>
		<array>
			<string>Utility/Logic</string>
		</array>
		<key>description</key>
		<string>Generates a pulse for a duration of a single frame whenever the input signal changes according to detection mode.</string>
		<key>name</key>
		<string>Pulse</string>
		<key>aliasNames</key>
		<array>
			<string>Signal</string>
			<string>Leading Edge</string>
			<string>Trailing Edge</string>
		</array>
		<key>examples</key>
        <array>
			<string>Pulse.qtz</string>
            <string>Mouse Scroll Reset.qtz</string>
        </array> 
	</dict>
	
	<key>inputAttributes</key>
	<dict>
		<key>inputMode</key>
		<dict>
			<key>description</key>
			<string>The detection mode for the input signal.</string>
			<key>name</key>
			<string>Detection Mode</string>
			<key>menu</key>
			<array>
				<string>Leading Edge</string>
				<string>Trailing Edge</string>
				<string>Both</string>
			</array>
		</dict>
