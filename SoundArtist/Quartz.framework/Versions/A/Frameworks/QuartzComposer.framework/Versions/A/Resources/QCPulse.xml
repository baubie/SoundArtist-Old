		<dict>
			<key>description</key>
			<string>The input signal.</string>
			<key>name</key>
			<string>Input Signal</string>
		</dict>
	</dict>
	
	<key>outputAttributes</key>
	<dict>
		<key>outputSignal</key>
		<dict>
			<key>description</key>
			<string>Generated pulse.</string>
			<key>name</key>
			<string>Pulse</string>
		</dict>
	</dict>
</dict>
