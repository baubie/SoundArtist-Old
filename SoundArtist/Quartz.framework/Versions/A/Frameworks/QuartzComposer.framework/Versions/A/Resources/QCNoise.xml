<plist version="1.0">
<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>categories</key>
		<array>
			<string>Utility/Numeric</string>
		</array>
		<key>description</key>
		<string>Given a position, the Noise patch outputs a random value based on a Perlin distribution.
		
The noise produced is deterministic and safe to use inside an iterator.</string>
		<key>name</key>
		<string>Noise</string>
		<key>aliasNames</key>
		<array>
			<string>Random</string>
		</array>
        <key>examples</key>
        <array>
            <string>Noise 3D.qtz</string>
        </array>  
	</dict>
		
	<key>outputAttributes</key>
	<dict>
		<key>outputValue</key>
		<dict>
			<key>description</key>
			<string>The resulting value.</string>
			<key>name</key>
			<string>Result</string>
		</dict>
	</dict>
	
	<key>_dynamicAttributes</key>
	<array>
		<dict>
			<key>inName</key>
			<string>X</string>
			<key>inDescription</key>
			<string>The X dimension in the noise.</string>
		</dict>
		<dict>
			<key>inName</key>
			<string>Y</string>
			<key>inDescription</key>
			<string>The Y dimension in the noise.</string>
		</dict>
		<dict>
			<key>inName</key>
			<string>Z</string>
			<key>inDescription</key>
			<string>The Z dimension in the noise.</string>
		</dict>
	</array>
</dict>
</plist>

